# Host vyos
#     StrictHostKeyChecking no
#     HostName 192.168.255.1
#     User vagrant
#     IdentityFile ~/.ssh/vagrant
#     Port 22

Host vyos
    HostName 192.168.255.1
    User vagrant
    Port 22
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    PasswordAuthentication no
    IdentityFile ~/.ssh/vagrant
    IdentitiesOnly yes
    LogLevel FATAL

Host InfSvc-CentOS
    StrictHostKeyChecking no
    HostName 192.168.255.11
    User vagrant
    IdentityFile ~/.ssh/vagrant
    Port 22

Host InfSvc-Debian
    StrictHostKeyChecking no
    HostName 192.168.255.12
    User vagrant
    IdentityFile ~/.ssh/vagrant
    Port 22

Host InfSvc-Alpine
    StrictHostKeyChecking no
    HostName 192.168.255.21
    User vagrant
    IdentityFile ~/.ssh/vagrant
    Port 22

Host InfSvc-Fedora
    HostName 192.168.255.31
    User vagrant
    Port 22
    UserKnownHostsFile /dev/null
    StrictHostKeyChecking no
    PasswordAuthentication no
    IdentityFile ~/.vagrant.d/insecure_private_key
    IdentitiesOnly yes
    LogLevel FATAL